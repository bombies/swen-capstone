{
	"name": "marketplace",
	"type": "module",
	"private": true,
	"packageManager": "bun@1.2.13",
	"scripts": {
		"dev": "sst dev",
		"build": "turbo build:wrapped --filter=@lumi/frontend -- --stage $STAGE",
		"deploy": "sst deploy",
		"typecheck": "turbo typecheck",
		"lint": "eslint \"**/*.+(ts|tsx)\"",
		"lint:fix": "eslint \"**/*.+(ts|tsx)\" --fix",
		"migrate-auth": "bunx sst shell  -- bunx @better-auth/cli generate --config packages/frontend/src/auth.ts && @better-auth/cli migrate"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"workspaces": [
		"packages/*"
	],
	"dependencies": {
		"@nestjs/cli": "^11.0.7",
		"sst": "3.17.3"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^4.14.1",
		"@eslint-react/eslint-plugin": "^1.51.2",
		"@next/eslint-plugin-next": "^15.3.3",
		"@types/aws-lambda": "8.10.149",
		"@types/bun": "latest",
		"@typescript-eslint/eslint-plugin": "^8.33.1",
		"@typescript-eslint/parser": "^8.33.1",
		"eslint": "^9.28.0",
		"eslint-plugin-format": "^1.0.1",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-react-hooks": "^6.0.0-rc.1",
		"eslint-plugin-react-refresh": "^0.4.20"
	}
}
